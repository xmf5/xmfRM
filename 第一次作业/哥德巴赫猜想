#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);
    
    for(int k=4; k <n;k+=2){
        for (int i = 2; i <=k; i++) {
            int isprime = 1; 
             
            for (int c = 2; c<i; c++) {
                if ( i%c==0) {
                    isprime= 0; 
                    break;
                }
            }
            int t = k- i;
            int isprim = 1; 
            for (int b = 2; b <t; b++) {
                if (t % b == 0) {
                    isprim = 0; 
                    break;
                }
            }
            if (isprime+isprim==2) {
                printf("%d=%d+%d\n", k, i, t);
                break;
            }
        }
    }
    return 0;
}
